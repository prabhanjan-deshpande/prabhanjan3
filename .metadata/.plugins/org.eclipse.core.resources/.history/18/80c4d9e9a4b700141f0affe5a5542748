package program;

import java.util.Scanner;

public class SumDigit {
	public static void main(String args[])
	{		
		int n, res, sum;
		Scanner scan= new Scanner(System.in);
		System.out.println("Please Enter No1: ");
		n1=scan.nextInt();
		System.out.println("Please Enter No1: ");
		n2=scan.nextInt();
		
		while(n>0)
		{
			res=n%10;
			n=n/10;
			sum+=res;	//sum=sum+res;
		}
		System.out.println("Second No: " + n2);

	}
}
